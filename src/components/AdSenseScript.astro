---
import { SITE } from "../config/site";
const client = SITE.adsenseClient?.trim();
---
{client ? (
  <script
    async
    id="adsense-js"
    src={`https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=${encodeURIComponent(client)}`}
    crossorigin="anonymous"
  ></script>
) : null}
